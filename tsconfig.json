{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "esnext",
     "baseUrl": ".",
    "paths": {
      "@src/*": ["./src/*"],
      "@assets/*": ["./assets/*"],
      "@api/*": ["./api/*"],
      "@assets_/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@constants/*": ["./src/constants/*"],
      "@helpers/*": ["./src/helper/*"],
      "@lib/*": ["./src/lib/*"],
      "@routes/*": ["./src/routes/*"],
      "@screens/*": ["./src/routes/*"],
      "@utils/*": ["./src/utils/*"],
      "@fonts": ["./src/assets/fonts/fonts.ts"],
      "@icons":["./src/assets/icons/index.tsx"],
    }
  },
  "types": ["react-native", "@types/react-native-dotenv"]
}
